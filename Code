from djitellopy import Tello

from time import sleep

tello = Tello()

tello.connect()
tello.takeoff()
# part 1
tello.move_up(101)

tello.move_forward(152)
tello.rotate_counter_clockwise(90)

# part 2
tello.move_forward(183)
tello.move_down(91)

tello.rotate_clockwise(90)
tello.move_forward(91)

tello.rotate_clockwise(90)
tello.move_forward(91)
tello.move_up(31)
# part 3

tello.rotate_counter_clockwise(90)
tello.move_forward(182)
tello.land()
